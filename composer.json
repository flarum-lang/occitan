{
	"name": "flarum-lang/occitan",
	"type": "flarum-extension",
	"description": "Occitan language pack for Flarum.",
	"homepage": "https://github.com/flarum-lang/occitan",
	"keywords": [
		"locale",
		"flarum",
		"flarum-locale",
		"oc-locale",
		"occitan-locale",
		"occitan"
	],
	"license": "MIT",
	"support": {
		"issues": "https://github.com/flarum-lang/occitan/issues",
		"source": "https://github.com/flarum-lang/occitan"
	},
	"require": {
		"flarum/core": "^1.6"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"flarum-extension": {
			"title": "Occitan",
			"category": "language",
			"icon": {
				"image": "icon.svg",
				"backgroundSize": "cover",
				"backgroundPosition": "center"
			}
		},
		"flarum-locale": {
			"code": "oc",
			"title": "Occitan"
		},
		"extiverse": {
			"discuss": ""
		}
	}
}
